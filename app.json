{
  "expo": {
    "name": "BoaHora | Monitore suas Contrações",
    "slug": "boa-hora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon.png",
    "scheme": "boahora",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/icons/splash-icon-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#6EE7B7"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6EE7B7",
        "foregroundImage": "./assets/icons/android-adaptive-icon.png",
        "backgroundImage": "./assets/icons/android-icon-background.png",
        "monochromeImage": "./assets/icons/android-icon-monochrome.png"
      },
      "splash": {
        "image": "./assets/icons/splash-icon-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#6EE7B7"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/boa-hora-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6EE7B7",
          "dark": {
            "backgroundColor": "#6EE7B7"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "621f0021-d1f6-4701-9a09-4d1ada00739b"
      }
    },
    "owner": "mazzoni-solutions",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/621f0021-d1f6-4701-9a09-4d1ada00739b"
    }
  }
}
